{
    "root": true,
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
     "rules": {
         "no-console": 0,
         "indent": [2, 4],
         "arrow-parens": 0,
         "generator-star-spacing": 0,
         "max-len": [2, 120, 4, {
             "ignoreUrls": true,
             "ignoreComments": true
         }]
     }
}
